<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".fragments.ResultFragment">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:paddingBottom="20sp"
        android:layout_height="match_parent">


        <androidx.appcompat.widget.Toolbar
            android:id="@+id/tooolbaradd"
            android:layout_width="match_parent"
            android:layout_height="60dp"
            android:background="#52CA6C"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:titleTextColor="@color/white" >

            <ImageView
                android:id="@+id/back"
                android:layout_width="38dp"
                android:layout_height="38dp"
                android:layout_marginStart="20sp"
                android:src="@drawable/ic_baseline_arrow_back_ios_24"
                android:layout_gravity="start"/>

            <TextView
                android:id="@+id/teks"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textStyle="normal"
                android:maxLines="1"
                android:ellipsize="end"
                android:layout_marginEnd="12dp"
                android:paddingEnd="12dp"
                android:layout_marginStart="12dp"
                android:textColor="@color/white"
                android:text="Sanjar Suvonov"
                android:textSize="19sp"/>

        </androidx.appcompat.widget.Toolbar>

        <ImageView
            android:id="@+id/rasm"
            android:layout_width="156dp"
            android:layout_height="102dp"
            android:scaleType="centerCrop"
            android:src="@drawable/image"
            android:layout_marginTop="38dp"
            app:layout_constraintTop_toBottomOf="@id/tooolbaradd"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"/>

        <TextView
            android:id="@+id/ism"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
           android:gravity="center"
            android:layout_marginTop="24dp"
            android:paddingEnd="12dp"
            android:text="Kadirov Shoxrux Shavkato'g'li"
            android:textColor="@color/black"
            android:textSize="24sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/rasm"
            app:layout_constraintVertical_bias="0.0" />

        <TextView
            android:id="@+id/malum"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="24dp"
            android:text="Fuqaroning shaxsiy malumotlari"
            android:textColor="@color/black"
            android:textSize="18sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/ism"
            app:layout_constraintVertical_bias="0.04000002" />

        <TextView
            android:id="@+id/vil"
            android:layout_width="0dp"
            android:paddingEnd="12dp"
            android:paddingStart="20dp"
            android:layout_height="wrap_content"
            android:text="Viloyati"
            android:textColor="@color/black"
            android:textSize="18sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.06"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/malum"
            app:layout_constraintVertical_bias="0.19999999" />


        <TextView
            android:id="@+id/shahri"
            android:layout_width="0dp"
            android:paddingStart="20sp"
            android:paddingEnd="12dp"
            android:maxLines="1"
            android:ellipsize="end"
            android:layout_height="wrap_content"
            android:layout_marginTop="24sp"
            android:text="Shahrisddddddddddddddddddddddddddddddddddddsdddd"
            android:textColor="@color/black"
            android:textSize="18sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.06"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/vil"
            app:layout_constraintVertical_bias="0.01999998" />

        <TextView
            android:id="@+id/timee"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="24sp"
            android:maxLines="1"
            android:ellipsize="end"
            android:paddingEnd="12dp"
            android:paddingStart="20dp"
            android:text="Passport olgan vaqti"
            android:textColor="@color/black"
            android:textSize="18sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.06"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/shahri"
            app:layout_constraintVertical_bias="0.0" />

        <TextView
            android:id="@+id/muddati"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="24sp"
            android:text="Passport muddatisdddddddddddddddddddddddddddddddddddddddddddddddddd"
            android:ellipsize="end"
            android:maxLines="1"
            android:textColor="@color/black"
            android:paddingEnd="12dp"
            android:paddingStart="20dp"
            android:textSize="18sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.06"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/timee"
            app:layout_constraintVertical_bias="0.0" />

        <TextView
            android:id="@+id/pol"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="24sp"
            android:text="Jinsi"
            android:textColor="@color/black"
            android:textSize="18sp"
            android:paddingEnd="12dp"
            android:paddingStart="20dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            android:paddingBottom="20sp"
            app:layout_constraintHorizontal_bias="0.06"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/muddati"
            app:layout_constraintVertical_bias="0.0" />


    </androidx.constraintlayout.widget.ConstraintLayout>

</ScrollView>